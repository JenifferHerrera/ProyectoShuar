<div class="container-login100" style="background-image: url('/img/s2.jpg');">
	<div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
		<form class="login100-form validate-form" action="/servicio/crear" method="POST">
			<span class="login100-form-title p-b-37">
				Registro Servicio
			</span>
			<input type="hidden" name="estado" value="true" />
			<div class="wrap-input100 validate-input m-b-20" data-validate="Ingres el nombre ">
				<input class="input100" type="text" name="nombre" placeholder="Ingrese el nombre ">
				<span class="focus-input100"></span>
			</div>
			<div>
				<input type="file" name="imagen" id="imagen" placeholder="Foto">
				<label class="login100-form-btn" for="imagen">Subir Foto</label>
				<span class="focus-input100"></span>
			</div>
			<br>
			<div class="container-login100-form-btn">
				<button class="login100-form-btn">
					Registrarse
				</button>
			</div>
		</form>
	</div>
</div>