	<!--===============================================================================================-->
	<script src="/src/public/vendor/bootstrap/js/popper.js"></script>
	<script src="/src/public/vendor/bootstrap/js/bootstrap.min.js"></script>
	<!--===============================================================================================-->
	<script src="/src/public/vendor/select2/select2.min.js"></script>
	<!--===============================================================================================-->
	<script src="/src/public/vendor/daterangepicker/moment.min.js"></script>
	<script src="/src/public/vendor/daterangepicker/daterangepicker.js"></script>
	<!--===============================================================================================-->
	<script src="/src/public/vendor/countdowntime/countdowntime.js"></script>
	<!--===============================================================================================-->
	<script src="/src/public/js/main.js"></script>


</body>

</html>